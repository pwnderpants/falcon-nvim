{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "diagnostics": {
        "globals": [
            "vim",
            "require",
            "pcall",
            "pairs",
            "ipairs",
            "next",
            "type",
            "tonumber",
            "tostring",
            "unpack",
            "string",
            "table",
            "math",
            "coroutine",
            "io",
            "os",
            "debug"
        ]
    },
    "workspace": {
        "library": {
            "vim": true,
            "neovim": true
        },
        "checkThirdParty": false
    },
    "telemetry": {
        "enable": false
    }
} 